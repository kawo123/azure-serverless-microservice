{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "cosmosDbDoc",
      "databaseName": "Products",
      "collectionName": "ProductRatings",
      "createIfNotExists": "true",
      "connectionStringSetting": "CosmosDBConnection"
    }
  ]
}